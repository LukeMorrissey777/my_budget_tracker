    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@mikro-orm/cli": "^5.6.3",
    "@mikro-orm/core": "^5.6.3",
    "@mikro-orm/migrations": "^5.6.3",
    "@mikro-orm/postgresql": "^5.6.3",
    "body-parser": "^1.20.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "http": "^0.0.1-security",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "2.0.0-beta.1"
